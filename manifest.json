{
    "manifest_version": 3,
    "name": "Wowhead CN Redirector",
    "version": "1.0",
    "description": "Redirects wowhead.com/item links to wowhead.com/cn/item",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}